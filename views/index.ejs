<script>
    let carousel = document.querySelector('.carousel');
    let carousel_items = carousel.children;
    let trans_items = document.querySelector('.trans_item').getAttribute("data-set").split(",");
    let i = 1;
    let j = 0;
    document.getElementById("trans_con").innerHTML = trans_items[i - 1];
    setInterval(() => {
        carousel_items[i - 2 >= 0 ? i - 2 : i == 1 ? carousel_items.length - 1 : carousel_items.length - 2].setAttribute('data-set', 'beforeactive');
        carousel_items[i - 1 >= 0 ? i - 1 : carousel_items.length - 1].setAttribute('data-set', 'beforeactive');
        carousel_items[i].setAttribute('data-set', 'active');
        carousel_items[i < 3 ? i + 1 : 0].setAttribute('data-set', 'afteractive');
        i + 1 > 3 ? i = 0 : i++;
        back();
    }, 3000);
    function type() {
        if (j < trans_items[i - 1 >= 0 ? i - 1 : trans_items.length - 1].length) {
            document.getElementById("trans_con").innerHTML += trans_items[i - 1 >= 0 ? i - 1 : trans_items.length - 1].charAt(j);
            j++;
            setTimeout(type, 1000 / trans_items[i - 1 >= 0 ? i - 1 : trans_items.length - 1].length);
        }
    }

    function back() {
        j = 0;
        let val = document.getElementById("trans_con").innerHTML;
        let val1 = val;
        if (val.length > 0) {
            const interval = setInterval(() => {
                val = val.substr(0, val.length - 1);
                document.getElementById("trans_con").innerHTML = val;
            }, 10);
            setTimeout(() => {
                clearInterval(interval);
                document.getElementById("trans_con").innerHTML = "";
                // type();
            }, 500);
        }
        setTimeout(() => {
            // clearInterval(interval);
            type();
        }, 500);
    }
    // document.addEventListener("visibilitychange", () => {
